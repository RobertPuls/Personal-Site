<section id="mountain">
  {{#parallax-content speed=-0.5 height=650}}
    {{parallax-sun}}
  {{/parallax-content}}
  {{#parallax-content speed=-0.1 height=990}}
  <div id="slide1" class="slide">
    <div class="mountain">
      {{mountain-top}}
    </div>
    <div class="mountain-two">
      {{mountain-top}}
    </div>
     <div class="mountain-three">
       {{mountain-top}}
    </div>
    <div class="mountain-four">
      <div class="mountain-top-small">
        <div class="mountain-cap-1s"></div>
        <div class="mountain-cap-2s"></div>
        <div class="mountain-cap-3s"></div>
      </div>
    </div>
    <div class="mountain-five">
      <div class="mountain-top-small">
        <div class="mountain-cap-1s"></div>
        <div class="mountain-cap-2s"></div>
        <div class="mountain-cap-3s"></div>
      </div>
    </div>
  </div>
  <div class="cloud"></div>
  {{/parallax-content}}
</section>

<section id="trees">
  {{!-- {{#parallax-content speed=0.2 height=650}} --}}
  <div class="shadow1"></div>
  {{tree-section id="tree-1" class="pos"}}
  {{tree-section id="tree-2" class="pos"}}
  {{tree-section id="tree0" class="pos"}}
  {{tree-section id="tree1" class="pos"}}
  {{tree-section id="tree2" class="pos"}}
  {{tree-section id="tree3" class="pos"}}
  {{tree-section id="tree4" class="pos"}}
  {{!-- {{/parallax-content}} --}}
</section>

<section>
  {{skill-board model=model}}
  {{camp-fire}}
</section>

<section id="tent">
  {{project-tent class="tent"}}
</section>
